<resources>


    <style name="WeekTab">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:paddingVertical">8dp</item>
        <item name="android:paddingHorizontal">16dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="WeekTab.Active">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:paddingVertical">8dp</item>
        <item name="android:paddingHorizontal">16dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorHighlight</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="FoodWeekTab">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:paddingVertical">8dp</item>
        <item name="android:paddingHorizontal">16dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorHighlight</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="AuthTitle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/subTitleTextSize</item>
        <item name="android:layout_marginTop">148dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">?attr/fontFamily</item>
    </style>

    <style name="AuthSubtitle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/subTitleTextSize</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">?android:attr/fontFamily</item>
    </style>


    <style name="WeightTrackEditText">
        <item name="android:padding">8dp</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textColorHint">?attr/hintTextColor</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:layout_margin">12dp</item>
        <item name="android:textSize">@dimen/body</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="AuthButton">
        <item name="android:backgroundTint">?attr/colorPrimary</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">?attr/fontFamily</item>
        <item name="elevation">5dp</item>
    </style>

    <style name="AuthLink">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SignOutLink">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="ScreenDescription">
        <item name="android:textSize">@dimen/subHeadingTextSize</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="HeadingText">
        <item name="android:textSize">@dimen/subHeadingTextSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">12dp</item>
    </style>


    <style name="FollowUsText">
        <item name="android:textSize">@dimen/body</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorSecondary</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">12dp</item>
    </style>

    <style name="StartupButton">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/startup_button_bg</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginBottom">80dp</item>
        <item name="android:elevation">6dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>


    <style name="ItemContainerStyle">
        <item name="android:background">@drawable/workout_set_card_past</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:elevation">6dp</item>
        <item name="android:layout_marginHorizontal">16dp</item>
        <item name="android:layout_marginVertical">8dp</item>
        <item name="android:clipToPadding">false</item>
    </style>


    <style name="StreakIconStyle">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:contentDescription">@string/strike_icon</item>
        <item name="android:scaleType">fitCenter</item>
    </style>


    <style name="StreakCount">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/subHeadingTextSize</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="HeadingStyle">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/headingTextSize</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="CalenderText" parent="HeadingStyle">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/subHeadingTextSize</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Name_Style" parent="HeadingStyle">
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">?android:attr/textColorHighlight</item>
    </style>


    <style name="WorkoutTextStyleBase">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="TextDayName_Past" parent="WorkoutTextStyleBase">
        <item name="android:textColor">?attr/workoutTextColorPast</item>
    </style>

    <style name="TextDayName_Today" parent="WorkoutTextStyleBase">
        <item name="android:textColor">?attr/workoutTextColorToday</item>
    </style>

    <style name="TextDayName_Future" parent="WorkoutTextStyleBase">
        <item name="android:textColor">?attr/workoutTextColorFuture</item>
    </style>

    <style name="Date">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/subHeadingTextSize</item>
    </style>

    <style name="LeftArrow">
        <item name="android:src">@drawable/outline_chevron_left_24</item>
    </style>

    <style name="RightArrow">
        <item name="android:src">@drawable/outline_chevron_right_24</item>
        <item name="android:background">?attr/colorOnBackground</item>
    </style>


    <style name="Day">
        <item name="android:textColor">?android:attr/textColorSecondary</item>
        <item name="android:textSize">@dimen/body</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="CalenderDate" parent="Date">
        <item name="android:textStyle">bold</item>
    </style>


    <style name="CaloriesRelatedText">
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/body</item>
    </style>

    <style name="WeightText" parent="CaloriesRelatedText">
        <item name="android:textStyle">bold</item>
    </style>


    <style name="TableHeaderText" parent="WeightText" />

    <style name="CalorieCount" parent="CaloriesRelatedText">
        <item name="android:textSize">@dimen/headingTextSize</item>
        <item name="android:textColor">?android:attr/textColorHighlight</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="RoundedImage">
        <item name="elevation">5dp</item>
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>


    <style name="WorkListItemDayNumberTextPast">
